{
    "blocks": [
      {
        "type": "prompt",
        "alignment": "left",
        "segments": [
          {
            "type": "path",
            "style": "plain",
            "foreground": "#83769c",
            "properties": {
              "postfix": "/",
              "home_icon": "~",
              "folder_separator_icon": " \uE0b1 ",
              "style": "folder"
            }
          },
          {
            "type": "git",
            "style": "plain",
            "foreground": "#96ffbf",
            "properties": {
              "display_status": true,
              "prefix": " on ",
              "postfix": " ",
              "ahead_color": "#ff80bb",
              "behind_color": "#96ffbf",
              "working_color": "#ff80bb",
              "staging_color": "#ffc180",
              "local_changes_color": "#ffc180",
              "ahead_and_behind_color": "#ff80bb",
              "color_background": false,
              "display_stash_count": true,
              "display_upstream_icon": true,
              "status_colors_enabled": true,
              "github_icon": "Ôêà "
            }
          }
        ]
      },
      {
          "type": "rprompt",
          "alignment": "right",
          "segments": [
            {
              "type": "executiontime",
              "style": "plain",
              "foreground": "#83769c",
              "properties": {
                "always_enabled": true
              }
            }
          ]
      },
      {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
          {
              "type": "exit",
              "style": "powerline",
              "foreground": "#96ffbf",
              "properties": {
                  "display_exit_code": false,
                  "always_enabled": true,
                  "error_color": "#ff80bb",
                  "prefix": "\u276F"
              }
          }
      ]
  }
    ],
    "final_space": true,
    "console_title": true,
    "console_title_style": "template",
    "console_title_template": "{{ .Shell }} in {{ .Folder }}"
  }